AxBA,qBAwBA;AACA,QAAA,KAAA,GAAA,IAAA;;AACA,QAAA,KAAA,GAAA,IAAA,CAAA,aAAA,EAAA;AACA,IAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,KAAA;;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,KAAA,CAAA,EAAA;AACA,MAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,KAFA,MAEA;AACA;AACA,MAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,OAAA,CAAA,GAAA,CAAA,cAAA,GAAA,yBAAA,EAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,IAAA,GAAA,QAAA,CAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,OAAA,GAAA,OAAA,CAAA,GAAA,CAAA,cAAA,GAAA,uBAAA,GAAA,KAAA,CAAA,IAAA,CAAA,OAAA;AACA,SAHA,MAGA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;AACA,OARA;AASA;AACA,GA1CA;AA2CA,EAAA,QAAA,EAAA;AACA,IAAA,aADA,2BACA;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,UAAA,UAAA,GAAA,EAAA;;AACA,cAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,UAAA,CAAA,MAAA;AACA,aAAA,CAAA;AACA,UAAA,UAAA,GAAA,IAAA;AACA;;AACA,aAAA,CAAA;AACA,UAAA,UAAA,GAAA,UAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,UAAA,CAAA,CAAA,CAAA;AACA;;AACA,aAAA,CAAA;AACA,UAAA,UAAA,GAAA,UAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,UAAA,CAAA,CAAA,CAAA,GAAA,KAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,UAAA,CAAA,CAAA,CAAA;AACA;AATA;;AAWA,aAAA,UAAA;AACA;AAhBA,GA3CA;AA6DA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,uBACA,GADA,EACA;AACA,aAAA,OAAA,CAAA,GAAA,CAAA,cAAA,GAAA,uBAAA,GAAA,GAAA;AACA;AAHA;AA7DA,CAAA","sourcesContent":["<template>\r\n    <header>\r\n        <div class=\"l-content\">\r\n            <el-button plain icon=\"el-icon-menu\" size=\"mini\" @click=\"handleMenu\"></el-button>\r\n\r\n                <h4 style=\"color:#fff\" v-text=\"getBreadCrumb\"></h4>\r\n        </div>\r\n        <div class=\"r-content\">\r\n            <el-dropdown trigger=\"click\" size=\"mini\">\r\n                <span class=\"el-dropdown-link\">\r\n                    <img :src=\"user.headPic\" class=\"user\" />\r\n                </span>\r\n                <el-dropdown-menu slot=\"dropdown\">\r\n                    <el-dropdown-item @click.native=\"logout\">退出登录</el-dropdown-item>\r\n                </el-dropdown-menu>\r\n            </el-dropdown>\r\n        </div>\r\n    </header>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        data(){\r\n            return{\r\n                user:{}\r\n            }\r\n        },\r\n        methods:{\r\n            handleMenu() {\r\n                let _this = this;\r\n                _this.$store.commit(\"collapseMenu\");\r\n            },\r\n            logout(){\r\n                let _this = this;\r\n                _this.$ajax.post(process.env.VUE_APP_SERVER + \"/admin/user/logout\", {token: _this.user.token}).then((response)=>{\r\n                    let resp = response.data;\r\n                    if(resp.code === 0){\r\n                        _this.user.token = \"\";\r\n                        SessionStorage.remove(SESSION_KEY_LOGIN_ADMIN);\r\n                        _this.$message.success(resp.msg);\r\n                        _this.$router.push(\"/login\");\r\n                    }\r\n                });\r\n            }\r\n        },\r\n        created(){\r\n            let _this = this;\r\n            let token = Tool.getLoginAdmin();\r\n            _this.user.token = token;\r\n            if (Tool.isEmpty(token)) {\r\n                _this.$router.push(\"/login\");\r\n            }else{\r\n                //用token去查询用户数据是否仍存在\r\n                _this.$ajax.post(process.env.VUE_APP_SERVER + \"/admin/user/check_login\", {token:token}).then((response)=>{\r\n                    let resp = response.data;\r\n                    if(resp.code === 0){\r\n                        _this.user = resp.data;\r\n                        _this.user.headPic = process.env.VUE_APP_SERVER + \"/photo/view?filename=\" + _this.user.headPic;\r\n                    }else{\r\n                        _this.$router.push(\"/login\");\r\n                    }\r\n                });\r\n            }\r\n        },\r\n        computed:{\r\n            getBreadCrumb(){\r\n                let _this = this;\r\n                let breadCrumb = \"\";\r\n                switch(_this.$store.state.tab.breadCrumb.length){\r\n                    case 0:\r\n                        breadCrumb = \"首页\";\r\n                        break;\r\n                    case 1:\r\n                        breadCrumb = breadCrumb + _this.$store.state.tab.breadCrumb[0];\r\n                        break;\r\n                    case 2:\r\n                        breadCrumb = breadCrumb + _this.$store.state.tab.breadCrumb[0] + \" / \" + _this.$store.state.tab.breadCrumb[1];\r\n                        break;\r\n                }\r\n                return breadCrumb;\r\n            }\r\n        },\r\n        filters:{\r\n            filterPhoto(img){\r\n                return process.env.VUE_APP_SERVER + \"/photo/view?filename=\" + img;\r\n            }\r\n        }\r\n    }\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n    header {\r\n        display: flex;\r\n        height: 100%;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n    }\r\n\r\n    .l-content {\r\n        display: flex;\r\n        align-items: center;\r\n        .el-button {\r\n            margin-right: 20px;\r\n        }\r\n    }\r\n\r\n    .r-content {\r\n        .user {\r\n            width: 40px;\r\n            height: 40px;\r\n            border-radius: 50%;\r\n        }\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}