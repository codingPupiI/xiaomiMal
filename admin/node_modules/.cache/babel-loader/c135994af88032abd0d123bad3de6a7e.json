A,WAAA;;AACA,MAAA,KAAA,CAAA,YAAA;AACA,KAjEA;AAkEA,IAAA,QAlEA,sBAkEA;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,UAAA,KAAA,CAAA,iBAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,gBAAA;;AACA,eAAA,KAAA;AACA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,EAAA,GAAA,KAAA,CAAA,iBAAA,CAAA,CAAA,EAAA,EAAA;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,KAAA,CAAA,iBAAA,CAAA,CAAA,EAAA,KAAA;AACA,MAAA,KAAA,CAAA,iBAAA,GAAA,IAAA;AACA,KA3EA;AA4EA,IAAA,QA5EA,sBA4EA;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,UAAA,KAAA,CAAA,iBAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,gBAAA;;AACA,eAAA,KAAA;AACA;;AACA,MAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,OAAA,CAAA,GAAA,CAAA,cAAA,GAAA,yBAAA,EAAA;AAAA,QAAA,EAAA,EAAA,KAAA,CAAA,iBAAA,CAAA,CAAA,EAAA;AAAA,OAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,IAAA,GAAA,QAAA,CAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,aAAA,GAAA,IAAA,CAAA,IAAA,CAAA,gBAAA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA;AACA,UAAA,KAAA,CAAA,qBAAA,GAAA,IAAA;AACA,SAJA,MAIA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,OATA;AAUA;AA5FA,GAtBA;AAoHA,EAAA,OApHA,qBAoHA;AACA,QAAA,KAAA,GAAA,IAAA;;AACA,IAAA,KAAA,CAAA,YAAA;AACA,GAvHA;AAwHA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,uBACA,KADA,EACA;AACA,cAAA,KAAA;AACA,aAAA,CAAA;AACA,iBAAA,KAAA;;AACA,aAAA,CAAA;AACA,iBAAA,SAAA;;AACA,aAAA,CAAA;AACA,iBAAA,SAAA;;AACA,aAAA,CAAA;AACA,iBAAA,KAAA;;AACA,aAAA,CAAA;AACA,iBAAA,KAAA;AAVA;AAYA,KAdA;AAeA,IAAA,WAfA,uBAeA,GAfA,EAeA;AACA,aAAA,OAAA,CAAA,GAAA,CAAA,cAAA,GAAA,uBAAA,GAAA,GAAA;AACA;AAjBA;AAxHA,CAAA","sourcesContent":["<template>\r\n    <div id=\"order-list\">\r\n        <div style=\"margin-top: 20px\">\r\n            <el-button type=\"success\" @click=\"openEdit\"><i class=\"el-icon-edit-outline\"></i>修改状态</el-button>\r\n            <el-button type=\"warning\" @click=\"openView\"><i class=\"el-icon-view\"></i>查看详情</el-button>\r\n            <el-button type=\"danger\" @click=\"openRemove\"><i class=\"el-icon-delete\"></i>删除</el-button>\r\n            <el-input style=\"width:200px;height:50px;padding-left: 20px;\" v-model=\"searchContent\" placeholder=\"输入订单号搜索...\"/>\r\n            <el-button style=\"margin-left: 5px;\" type=\"primary\" @click=\"getOrderList\">搜索</el-button>\r\n        </div>\r\n        <el-table\r\n                ref=\"multipleTable\"\r\n                :data=\"tableData\"\r\n                tooltip-effect=\"dark\"\r\n                style=\"width: 100%\"\r\n                @selection-change=\"handleSelectionChange\">\r\n            <el-table-column\r\n                    type=\"selection\"\r\n                    width=\"50\">\r\n            </el-table-column>\r\n            <el-table-column\r\n                    prop=\"id\"\r\n                    label=\"编号\"\r\n                    width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column\r\n                    prop=\"tradeNo\"\r\n                    label=\"订单号\"\r\n                    width=\"200\">\r\n            </el-table-column>\r\n            <el-table-column\r\n                    prop=\"totalPrice\"\r\n                    label=\"订单金额(元)\"\r\n                    width=\"180\" align=\"center\">\r\n            </el-table-column>\r\n            <el-table-column\r\n                    prop=\"userDTO.username\"\r\n                    label=\"所属用户\"\r\n                    width=\"180\">\r\n            </el-table-column>\r\n            <el-table-column\r\n                    label=\"订单状态\"\r\n                    width=\"180\">\r\n                <template slot-scope=\"scope\">\r\n                    <span>{{scope.row.state|filterState}}</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column\r\n                    prop=\"createTime\"\r\n                    label=\"下单时间\"\r\n                    show-overflow-tooltip>\r\n            </el-table-column>\r\n        </el-table>\r\n        <el-pagination\r\n                background\r\n                layout=\"prev, pager, next\"\r\n                :total=\"pageData.total\"\r\n                :page-size=\"pageData.size\"\r\n                :current-page=\"pageData.page\"\r\n                @current-change=\"changePage\"\r\n                style=\"padding-top:10px;\">\r\n        </el-pagination>\r\n        <el-dialog title=\"修改订单状态信息\" :visible.sync=\"dialogFormVisible\">\r\n            <el-form :model=\"form\">\r\n                <el-form-item label=\"订单状态\" label-width=\"120px\">\r\n                    <el-select v-model=\"form.state\" placeholder=\"请选择订单状态\">\r\n                        <el-option label=\"未支付\" :value=\"1\"></el-option>\r\n                        <el-option label=\"已支付，待发货\" :value=\"2\"></el-option>\r\n                        <el-option label=\"已发货，待收货\" :value=\"3\"></el-option>\r\n                        <el-option label=\"已收货\" :value=\"4\"></el-option>\r\n                        <el-option label=\"已取消\" :value=\"5\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"dialogFormVisible=false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"saveOrder\">确 定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n\r\n        <el-dialog title=\"订单详情信息\" :visible.sync=\"viewDialogFormVisible\">\r\n            <span v-text=\"'订单号：'+order.tradeNo\"></span><br>\r\n            <span v-text=\"'收货地址：'+order.address\"></span><br>\r\n            <span v-text=\"'收货人姓名：'+order.receiverName\"></span><br>\r\n            <span v-text=\"'收货人电话：'+order.receiverPhone\"></span><br>\r\n            <span v-text=\"'收货人电子邮箱：'+order.receiverEmail\"></span><br>\r\n            <span v-text=\"'订单备注：'+order.remark\"></span>\r\n            <el-table :data=\"orderItemList\">\r\n                <el-table-column property=\"productName\" label=\"商品名称\" width=\"250\"></el-table-column>\r\n                <el-table-column align=\"center\" property=\"productPrice\" label=\"商品价格(元)\" width=\"100\"></el-table-column>\r\n                <el-table-column align=\"center\" property=\"quantity\" label=\"购买数量\" width=\"100\"></el-table-column>\r\n                <el-table-column align=\"center\" label=\"商品图片\" width=\"100\">\r\n                    <template slot-scope=\"scope\">\r\n                        <img :src=\"scope.row.productPhoto|filterPhoto\" style=\"width:90px;height:70px;\" />\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column align=\"center\" property=\"sum\" label=\"小计(元)\" show-overflow-tooltip></el-table-column>\r\n            </el-table>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button type=\"primary\" @click=\"viewDialogFormVisible=false\">确 定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\n    export default {\r\n        name: 'order-list',\r\n        data(){\r\n            return{\r\n                tableData: [],\r\n                multipleSelection: [],\r\n                dialogFormVisible: false,\r\n                viewDialogFormVisible: false,\r\n                form: {\r\n                    id: \"\",\r\n                    state: \"\"\r\n                },\r\n                searchContent: \"\",\r\n                pageData: {\r\n                    page: 1,\r\n                    size: 5,\r\n                    total: 0\r\n                },\r\n                order: {},\r\n                orderItemList: []\r\n            }\r\n        },\r\n        methods:{\r\n            saveOrder(){\r\n                let _this = this;\r\n                _this.$ajax.post(process.env.VUE_APP_SERVER + \"/admin/order/edit-state\", _this.form).then((response)=>{\r\n                    let resp = response.data;\r\n                    if(resp.code === 0){\r\n                        _this.$message.success(resp.msg);\r\n                        _this.dialogFormVisible = false;\r\n                        _this.getOrderList();\r\n                    }else{\r\n                        _this.$message.error(resp.msg);\r\n                    }\r\n                });\r\n            },\r\n            openRemove(){\r\n                let _this = this;\r\n                if(_this.multipleSelection.length !== 1){\r\n                    _this.$message.warning(\"请选择一条数据进行删除！\");\r\n                    return false;\r\n                }\r\n                _this.form.id = _this.multipleSelection[0].id;\r\n                this.$confirm('确定要删除编号为：' + _this.form.id + ' 的记录吗?', '提示', {\r\n                    confirmButtonText: '确定',\r\n                    cancelButtonText: '取消',\r\n                    type: 'warning'\r\n                }).then(() => {\r\n                    _this.removeOrder();\r\n                });\r\n            },\r\n            removeOrder(){\r\n                let _this = this;\r\n                _this.$ajax.post(process.env.VUE_APP_SERVER + \"/admin/order/remove\", _this.form).then((response)=>{\r\n                    let resp = response.data;\r\n                    if(resp.code === 0){\r\n                        _this.$message.success(resp.msg);\r\n                        _this.getOrderList();\r\n                    }else{\r\n                        _this.$message.error(resp.msg);\r\n                    }\r\n                });\r\n            },\r\n            handleSelectionChange(val) {\r\n                let _this = this;\r\n                _this.multipleSelection = val;\r\n            },\r\n            getOrderList(){\r\n                let _this = this;\r\n                let data = {\r\n                    searchContent: _this.searchContent,\r\n                    page: _this.pageData.page\r\n                };\r\n                _this.$ajax.post(process.env.VUE_APP_SERVER + \"/admin/order/list\", data).then((response)=>{\r\n                    let resp = response.data;\r\n                    if(resp.code === 0){\r\n                        _this.tableData = resp.data.list;\r\n                        _this.pageData.page = resp.data.page;\r\n                        _this.pageData.size = resp.data.size;\r\n                        _this.pageData.total = resp.data.total;\r\n                    }\r\n                });\r\n            },\r\n            changePage(currentPage){\r\n                let _this = this;\r\n                _this.pageData.page = currentPage;\r\n                _this.getOrderList();\r\n            },\r\n            openEdit(){\r\n                let _this = this;\r\n                if(_this.multipleSelection.length !== 1){\r\n                    _this.$message.warning(\"请选择一条数据进行修改状态！\");\r\n                    return false;\r\n                }\r\n                _this.form.id = _this.multipleSelection[0].id;\r\n                _this.form.state = _this.multipleSelection[0].state;\r\n                _this.dialogFormVisible = true;\r\n            },\r\n            openView(){\r\n                let _this = this;\r\n                if(_this.multipleSelection.length !== 1){\r\n                    _this.$message.warning(\"请选择一条数据进行查看详情！\");\r\n                    return false;\r\n                }\r\n                _this.$ajax.post(process.env.VUE_APP_SERVER + \"/admin/order/order-item\", {id:_this.multipleSelection[0].id}).then((response)=>{\r\n                    let resp = response.data;\r\n                    if(resp.code === 0){\r\n                        _this.orderItemList = resp.data.orderItemDTOList;\r\n                        _this.order = resp.data;\r\n                        _this.viewDialogFormVisible = true;\r\n                    }else{\r\n                        _this.$message.error(resp.msg);\r\n                    }\r\n                });\r\n            }\r\n        },\r\n        mounted(){\r\n            let _this = this;\r\n            _this.getOrderList();\r\n        },\r\n        filters:{\r\n            filterState(state){\r\n                switch(state){\r\n                    case 1:\r\n                        return '未支付';\r\n                    case 2:\r\n                        return '已支付，待发货';\r\n                    case 3:\r\n                        return '已发货，待收货';\r\n                    case 4:\r\n                        return '已收货';\r\n                    case 5:\r\n                        return '已取消';\r\n                }\r\n            },\r\n            filterPhoto(img){\r\n                return process.env.VUE_APP_SERVER + \"/photo/view?filename=\" + img;\r\n            }\r\n        }\r\n    }\r\n\r\n</script>\r\n"],"sourceRoot":"src/views"}]}